# Frontend Environment Variables
# Copy this file to .env.local and adjust values as needed.
# NOTE: Only variables prefixed with NEXT_PUBLIC_ are exposed to the browser.

# Base URL for the backend API (no trailing slash). For local dev with Docker:
# NEXT_PUBLIC_API_URL=http://localhost:8000
# For production (example):
# NEXT_PUBLIC_API_URL=https://api.example.com
NEXT_PUBLIC_API_URL=http://localhost:8000

# (Optional) Enable React Query devtools in production preview environments
# NEXT_PUBLIC_ENABLE_RQ_DEVTOOLS=false
